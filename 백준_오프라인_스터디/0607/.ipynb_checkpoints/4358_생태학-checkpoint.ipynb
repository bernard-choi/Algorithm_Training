    "import sys\n",
    "\n",
    "sajun = {}\n",
    "\n",
    "data = sys.stdin.read()\n",
    "tree = data.split('\\n')\n",
    "T_all = 0\n",
    "for t in tree:\n",
    "    if t != '':\n",
    "        T_all +=1\n",
    "        sajun[t] = sajun.get(t,0) + 1\n",
    "    \n",
    "\n",
    "\n",
    "\n",
    "sajun = sorted(sajun.items())\n",
    "\n",
    "for t,v in sajun:\n",
    "    print(\"%s %.4f\"%(t,v/T_all*100))\n",
    "\n",
    "\n",
    "\n",
    "\n"
   ]
  }
