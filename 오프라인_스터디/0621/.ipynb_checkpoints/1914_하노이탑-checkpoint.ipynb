   "source": [
    "i = int(input())\n",
    "\n",
    "def hanoi(n,fr,to,via):\n",
    "    if n == 1:\n",
    "        print(fr, to)\n",
    "    \n",
    "    else:\n",
    "        hanoi(n-1,fr,via,to)\n",
    "        hanoi(1,fr,to,via)\n",
    "        hanoi(n-1,via,to,fr)\n",
    "\n",
    "\n",
    "\n",
    "def hanoi_count(n):\n",
    "    print((2**n)-1)\n",
    "\n",
    "    \n",
    "hanoi_count(i)\n",
    "if i <= 20:\n",
    "    hanoi(i,1,3,2)\n",
    "    \n"
   ]
  }
